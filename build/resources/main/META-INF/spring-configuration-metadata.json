{
  "properties": [
    {
      "name": "spring.pulsar.service-url",
      "type": "java.lang.String",
      "description": "pulsar host."
  },
    {
      "name": "spring.pulsar.listener-name",
      "type": "java.lang.String",
      "description": "pulsar host.",
      "defaultValue": "UUID.randomUUID().toString()"
    },
    {
      "name": "spring.pulsar.io-threads",
      "type": "java.lang.Integer",
      "description": "pulsar io threads.",
      "defaultValue": 1
    },
    {
      "name": "spring.pulsar.listener-threads",
      "type": "java.lang.Integer",
      "description": "pulsar listener threads.",
      "defaultValue": 1
    },
    {
      "name": "spring.pulsar.enable-tcp-no-delay",
      "type": "java.lang.Boolean",
      "description": "pulsar tcp no delay.",
      "defaultValue": true
    },
    {
      "name": "spring.pulsar.connection-timeout",
      "type": "java.lang.Integer",
      "description": "pulsar connect timeout, unit million second.",
      "defaultValue": 5000
    },
    {
      "name": "spring.pulsar.keep-alive-interval",
      "type": "java.lang.Integer",
      "description": "pulsar connect keep-alive-interval, unit million second.",
      "defaultValue": 30000
    },
    {
      "name": "spring.pulsar.operation-timeout",
      "type": "java.lang.Integer",
      "description": "pulsar operation-timeout, unit million second.",
      "defaultValue": 30000
    }

] }